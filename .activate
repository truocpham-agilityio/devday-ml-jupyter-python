#!/usr/bin/env bash

################################################################################################################
## SCRIPTS
################################################################################################################

# This script install needed packages for the current environment
function venv-install() {
    pip install -e .
}

# This script run a Jupyter Notebook
function serve-notebook() {
    jupyter notebook --notebook-dir=notebooks
}

# This script run a Django Rest Framework Machine Learning
function run-server() {
    python house_price_ml/manage.py runserver
}

# This scirpt run a webapp
function run-webapp {
    yarn start
}
